#RewriteRule ^(.*)$ http://mte.spaceshiplabs.com/$1 [R=301,L]

RewriteBase /
#RewriteRule ^sienlace$ http://www.mejoratuescuela.org/peticiones/sienlace [QSA,R,L]
#RewriteRule ^califica-tu-escuela/?$ ?controler=compara [QSA,L]
RewriteRule ^sienlace/?$ http://www.mejoratuescuela/peticiones/sienlace [R=301,L]
RewriteRule ^([-_\w]+)/?$ ?controler=$1 [QSA,L]
RewriteRule ^([-_\w]+)/([-_\w]+)/?$ ?controler=$1&action=$2 [QSA,L]
RewriteRule ^([-_\w]+)/([-_\w]+)/([-_\w]+)/?$ ?controler=$1&action=$2&id=$3 [L]
RewriteRule ^([-_\w]+)/([-_\w]+)/([-_\w]+)/e=(\w)+/?$ ?controler=$1&action=$2&id=$3&error=$4 [L]

