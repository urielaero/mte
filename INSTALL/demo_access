php_value magic_quotes_gpc Off
php_value max_execution_time 200
php_value max_input_time 200

RewriteEngine On

RewriteCond %{HTTP_HOST} sienlace2014\.(com|com\.mx|org\.mx|mx|org)
RewriteCond %{HTTP_HOST}   !^$
RewriteRule ^(.*)$ http://www.mejoratuescuela/peticiones/sienlace [R=301,L]

